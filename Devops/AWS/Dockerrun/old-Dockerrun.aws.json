{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "renanj/viradasaude:v6-front",
      "hostname": "frontend",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["api"]
    },
    {
      "name": "api",
      "image": "renanj/viradasaudeapi:v6-front",
      "hostname": "api",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8000,
          "containerPort": 8000
        }
      ]
    }
  ]
}